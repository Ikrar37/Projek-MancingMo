{
  "version": 2,
  "builds": [
    {
      "src": "ecommerce/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/static.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/static.py?path=$1"
    },
    {
      "src": "/(.*)",
      "dest": "ecommerce/wsgi.py"
    }
  ]
}